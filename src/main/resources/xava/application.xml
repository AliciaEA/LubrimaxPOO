<?xml version = "1.0" encoding = "ISO-8859-1"?>

<!DOCTYPE application SYSTEM "dtds/application.dtd">

<application name="LubrimaxPOO"> 
	
	<!-- 
	A default module for each entity is assumed with the
	controllers on <default-module/>
	-->
	<default-module>		  
		<controller name="Typical"/>		
	</default-module>
	
	<!-- Módulos explícitos eliminados para evitar duplicados en el menú.
	     OpenXava generará automáticamente un módulo por cada entidad usando
	     el controlador Typical según la configuración de default-module. -->

</application>
